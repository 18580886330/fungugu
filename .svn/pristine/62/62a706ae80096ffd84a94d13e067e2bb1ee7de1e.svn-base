<div id="scrollWamp">
	<div class="wamp-body user-info" id="user_info">
		<p class="g-title">基本信息</p>
		<div class="user-info-hd">
			<div class="lump lump-1">
				<p class="fgg-icon man" id="sex_man">&#xe61e;</p>
				<p class="fgg-icon woman hide" id="sex_woman">&#xe61f;</p>
				<p class="user-name" id="userct_name">&nbsp;</p>
				<p class="user-lever">
					<span class="grade">普通会员</span>
					<i class="fgg-icon">&#xe620;</i>
				</p>
				<p>
					<span id="user_expire">&nbsp;</span>&nbsp;到期
				</p>
				<a class="c-blue" id="updatePass" href="javascript:;">修改密码</a>
			</div>
			<div class="lump lump-2">
				<p class="s-title">房产估值统计</p>
				<p>个人累积估值条数</p>
				<p id="person_surplus">&nbsp;</p>
				<div class="hide" id="mechanism_count">
					<p>机构剩余条数</p>
					<p id="mechanism_surplus">&nbsp;</p>
				</div>
				<p><a class="c-blue hide" id="more_outfit" href="javascript:;">更多</a></p>
			</div>
			<div class="lump lump-3">
				<div class="xujia_charts" id="inquiry_line"></div>
				<div class="share-outfit" id="share_outfit"> 
					<a class="c-blue outfit-back" id="outfit_back" href="javascript:;">返回</a>
					<ul class="outfit-list" id="outfitList"></ul>
					<div class="outfit-pages" id="outfitPages"></div>
				</div>
			</div>
		</div>
		<p class="g-title">修改个人信息</p>
		<div class="layui-form user-info-bd" id="updateUserInfo">
			<div class="layui-form-item">
				<div class="layui-inline">
					<label class="layui-form-label">
						姓名
					</label>
					<div class="auto-input-inline">
						<input type="text" class="layui-input" id="user_uName" autocomplete="off" maxlength="20">
					</div>
				</div>
				<div class="layui-inline">
					<label class="layui-form-label">
						用户名
					</label>
					<div class="auto-input-inline">
						<input type="text" class="layui-input" id="user_uLoginName" disabled="disabled" />
					</div>
				</div>
			</div>
			<div class="layui-form-item">
				<div class="layui-inline">
					<label class="layui-form-label">
						性别
					</label>
					<div class="auto-input-inline">
						<input type="radio" name="sex" value="男" title="男">
      					<input type="radio" name="sex" value="女" title="女">
					</div>
				</div>
				<div class="layui-inline">
					<label class="layui-form-label">
						邮箱
					</label>
					<div class="auto-input-inline">
						<input type="text" class="layui-input" id="user_uEmail" autocomplete="off" >
					</div>
				</div>
			</div>
			<div class="layui-form-item">
				<div class="layui-inline">
					<label class="layui-form-label">
						手机
					</label>
					<div class="auto-input-inline" id="phone_step1">
						<input type="text" class="layui-input" id="user_uPhone" autocomplete="off" maxlength="11">
						<a class="layui-btn prov-btn" id="prov_phone">绑定手机</a>
					</div>
					<div class="auto-input-inline hide" id="phone_step2">
						<a class="auth" id="changenImgCode" href="javascript:;" title="看不清,换一张">
							<img  id="img_code" />
						</a>
						<input type="text" class="layui-input yzm-input" id="yzm_code" placeholder="请输入图片验证码" autocomplete="off" maxlength="4">
						<a class="layui-btn prov-btn yzm-btn" id="verityCode">下一步</a>
					</div>
					<div class="auto-input-inline hide" id="phone_step3">
						<input type="text" class="layui-input fl mid-inpt" id="newPhone" placeholder="请输入新手机号" maxlength="11"/>
						<input type="text" class="layui-input fl mid-inpt" id="phoneYzm" placeholder="请输入短信验证号" autocomplete="off" maxlength="10"/>
						<a class="layui-btn prov-btn sep-btn" id="sendYzm">发送短信验证码</a>
					</div>
					
					
				</div>
				<div class="layui-inline">
					<label class="layui-form-label">
						 机构
					</label>
					<div class="auto-input-inline">
						<input type="text" class="layui-input" id="user_uOutfit" disabled="disabled">
					</div>
				</div>
			</div>
			<div class="layui-form-item">
				<div class="layui-inline">
					<label class="layui-form-label">
						部门
					</label>
					<div class="auto-input-inline">
						<input type="text" class="layui-input" id="user_uBranch" autocomplete="off" length="15" onkeyup="yf.limitLength(this)"/>
					</div>
				</div>
				<div class="layui-inline">
					<label class="layui-form-label">
						  职业
					</label>
					<div class="auto-input-inline">
						<input type="text" class="layui-input" id="user_uJob" autocomplete="off" length="20" onkeyup="yf.limitLength(this)"/>
					</div>
				</div>
			</div>
			<div class="layui-form-item">
				<div class="layui-inline">
					<label class="layui-form-label">
						区域
					</label>
					<div class="auto-input-inline">
						<div class="mid-sel">
							<select id="user_cur_city" lay-filter="curCity" >
								<option></option>
							</select>
						</div>
						<label class="inline min-label">&nbsp;</label>
						<div class="mid-sel">
							<select id="user_cur_district" lay-filter="districts">
								<option></option>
							</select>
						</div>
					</div>
				</div>
				<div class="layui-inline">
					<label class="layui-form-label">
						   地址
					</label>
					<div class="auto-input-inline">
						<input type="text" class="layui-input" id="user_uAddr" autocomplete="off" length="60" onkeyup="yf.limitLength(this)"/>
					</div>
				</div>
			</div>
		</div>
		<p class="text-center"><a class="layui-btn" id="subInfo">确认提交</a></p>
	</div>
</div>
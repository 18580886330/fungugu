						<div class="auto-input-inline">
							<select id="valuation_organ" lay-filter="valuation-organ"  lay-search>
								<option></option>
							</select>
						</div>
					</div>
					<div class="layui-inline">
						<label class="layui-form-label">日期范围</label>
						<div class="auto-input-inline">
							<input class="layui-input min-input-sm fl" id="begin_date" readonly="readonly"/>
							<input class="layui-input min-input-sm fr" id="end_date" readonly="readonly"/>
						</div>
					</div>
					<div class="layui-inline">
						<label class="layui-form-label">面积范围</label>
						<div class="auto-input-inline">
							<div class="min-input">
								<input class="layui-input" id="min_area" placeholder="最小面积"/>

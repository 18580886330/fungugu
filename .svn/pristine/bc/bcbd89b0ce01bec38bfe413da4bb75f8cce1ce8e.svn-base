        td{border: none;border-right: 1px solid #e2e2e2;border-bottom: 1px solid #e2e2e2;}
    }
   .preview-page-fd{text-align: center;color: #d84a38;font-size: 16px;font-weight: 700;
        .title{font-size: 20px;font-weight: 700;color: #000;margin-top: 20px;}    
        p{margin-bottom: 15px;}
        .photo-list{padding: 0 15px;
            li{display: table;float: left; margin: 0 4px 8px; width: 48.6%; height: 190px; position: relative; border: solid 1px #ccc; overflow: hidden; text-align: center;box-sizing: border-box; 
                span{display: table-cell;vertical-align: middle;height: 190px;}
                img{max-width: 100%; max-height: 90%;}
            }
        }
   }
}
/*账号管理 添加账号弹窗*/
.account-pop{width: 420px;display: none;
    .pop-body{height: auto;padding-left: 0;
        .last-item{margin-top: 16px;}
        .layui-form{margin-top: 10px;}
        .layui-input-block{margin-left: 80px;
            .layui-btn{padding: 0 27px;}
        }
    }
}
/*删除的弹窗*/
.routine-pop{width: 266px;background: #ffffff;height: 192px;display: none;
    .report-affirm-info {padding: 30px;
        .report-affirm-describle{width: 142px;font-size: 14px;margin-top: 20px;}
    }
    .report-affirm-fd{margin-left: 8px;
        .layui-btn{padding: 0 30px;}
    }
}
/*权限管理*/
.authority-cont{
   .layui-form{
        .layui-form-item{margin-bottom: 0;
            .layui-input{padding: 4px;}
        }
        a{@extend .inline;}
        .authority-reveal{height: 38px;line-height: 38px;}
        .authority-unit{position: absolute;top:10px;right: -14px;}
    }
}
.authority-condition{
    .layui-form-select dl{max-height: 190px;}
    .mCSB_container.mCS_no_scrollbar_y.mCS_y_hidden{margin-right: 0;}
}
/*批量弹窗*/
.batch-pop{width: 587px;display: none;overflow: hidden;
    .pop-body{height: 272px;padding-top: 24px;}
    .layui-form-label{text-align: right;width: 90px;}
    .auto-input-inline{margin-left: 90px;}
    .layui-form-item .layui-inline.left-inline{margin-right: 26px;}
    .batch-pop-fd{text-align: center;margin-top: 20px;padding-bottom: 20px;}
    .layui-form-select dl{max-height: 126px;}
    .batch-pop-unit{position: absolute;right: 10px;top:10px}
    .mCSB_container.mCS_no_scrollbar_y.mCS_y_hidden{margin-right: 0;}
}
/*收索提示弹窗*/
.search-pop{border-radius:10px;background: #fff;width: 360px;height: auto;position: relative;padding-top: 20px;overflow: hidden;display: none;
    .close {position: absolute;right: 10px;top: 10px;}
    .search-pop-info{padding-left: 32px;height: auto;width: auto;
        .fgg-icon{color: #ff3209;font-size: 38px;}
        .search-pop-bd{margin-top: 24px;}
        .search-pop-describle{line-height: 16px;width: 233px;margin-top: 0!important;font-size: 13px;color: #666;margin-left: 24px;overflow: hidden; word-break: break-all;}
        .search-btns{margin: 25px 0 36px 44px;width: 210px;}
    }
    .search-btns{
        .layui-btn{height: 25px;width: 74px;padding: 0;line-height: 25px;font-size: 12px;}
    }
    .search-pop-fd{color: #999;padding-bottom: 5px;
        .fgg-icon{color: #51cfd0;}
        .icon-popup-stip{width: 10px;height: 13px;margin: 0 6px 0 12px;background-position:-43px -247px;@extend .inline;}
        .c-blue{
            &:hover{text-decoration:underline;color: #3E8EDE;}
        }
        .c-gray:hover{color: #ccc;}
    }
}

@media screen and (max-width: 1300px) {
    .layui-table th, .layui-table td{ font-size: 12px; padding: 9px 12px; }
    .layui-laypage a,
    .layui-laypage span{ padding: 0 10px; }
}

@keyframes changeColor
{
    0% {background-color:#dffcfd;}
    33% {background-color:#fff;}
    66% {background-color:#dffcfd;}
    100% {background-color:#fff;}
}

@-moz-keyframes changeColor /* Firefox */
{
    0% {background-color:#dffcfd;}
    33% {background-color:#fff;}
    66% {background-color:#dffcfd;}
    100% {background-color:#fff;}
}

@-webkit-keyframes changeColor /* Safari 和 Chrome */
{
    0% {background-color:#dffcfd;}
    33% {background-color:#fff;}
    66% {background-color:#dffcfd;}
    100% {background-color:#fff;}
}